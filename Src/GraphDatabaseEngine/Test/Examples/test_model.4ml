domain TestModel
{
	A ::= new (Integer).
	B ::= new (id: String).
	C ::= new (x: Integer, y: Integer).
	D ::= new (x: String, y: A + Integer + {"hello", "world"}).
	E ::= A + B + C + D.

}

domain TestModelPlus extends TestModel
{
	C(a, c) :- C(a, b), C(b, c), c = 3.
}

model m of TestModel
{
	A(1).
	a1 is A(2).
	b1 is B("world").
	c1 is C(1, 2).
	c2 is C(2, 3).
}

model mplus of TestModelPlus
{	
	A(1).
	a1 is A(2).
	
	c1 is C(1, 2).
	c2 is C(2, 3).
	c3 is C(3, 4).
	d1 is D("hello", "world"). 
	d2 is D("hello", 1). 
	d3 is D("hello", a1). 
}
